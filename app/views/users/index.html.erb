<% provide(:title, "users") %>
<%= search_form_for @search, url: user_search_path do |f| %>
  <div class="searchbox">
    <%= f.text_field :name_cont, placeholder: "名前で検索..."  %>
  </div>
  <%= f.submit "検索", class: "btn btn-primary" %>
<% end %>
<h1>ユーザー一覧</h1>

<ul class="users">
  <%= render @users %>
</ul>

<%= will_paginate %>
